    "node": "20"
  },
  "main": "lib/index.js",
  "dependencies": {
    "@google-cloud/speech": "^6.0.0",
    "@google/generative-ai": "^0.21.0",
    "firebase-admin": "^12.0.0",
    "firebase-functions": "^7.0.4",
    "nodemailer": "^6.9.0"
  },
  "devDependencies": {
    "@types/nodemailer": "^6.4.14",
    "firebase-functions-test": "^3.0.0",
    "typescript": "^5.0.0"
  },
  "private": true
}
